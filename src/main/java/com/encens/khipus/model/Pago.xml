<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

        <entity class="com.encens.khipus.model.Pago">
                <table name="pago" schema="" catalog="khipus"/>
                <attributes>
                        <id name="idPago">
                                <column name="id_pago" nullable="false" precision="19"/>
                        </id>
                        <basic name="fecha">
                                <column name="fecha" nullable="false" precision="19"/>
                        </basic>
                        <basic name="monto">
                                <column name="monto" precision="22"/>
                        </basic>
                        <basic name="descripcion">
                                <column name="descripcion" length="150"/>
                        </basic>
                        <many-to-one name="sfConfencByIdSfConfenc" optional="true"
                                     target-entity="com.encens.khipus.model.SfConfenc">
                                <join-column name="id_sf_confenc" nullable="false"
                                             referenced-column-name="id_sf_confenc"/>
                        </many-to-one>
                </attributes>
        </entity>
</entity-mappings>
