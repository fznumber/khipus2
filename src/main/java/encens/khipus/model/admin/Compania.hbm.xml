<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.admin.Compania" table="compania" schema="" catalog="eos">
        <id name="idcompania">
            <column name="IDCOMPANIA" sql-type="bigint" not-null="true"/>
        </id>
        <property name="codigo">
            <column name="CODIGO" sql-type="varchar"/>
        </property>
        <set name="usuariosByIdcompania" inverse="true">
            <key>
                <column name="IDCOMPANIA"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.admin.Usuario"/>
        </set>
    </class>
</hibernate-mapping>