<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.ArticulosPaquete" table="articulos_paquete" schema="" catalog="eos">
        <id name="idarticulospaquete">
            <column name="IDARTICULOSPAQUETE" sql-type="bigint" not-null="true"/>
        </id>
        <property name="idpaquetes">
            <column name="IDPAQUETES" sql-type="bigint"/>
        </property>
        <property name="idCuenta">
            <column name="ID_CUENTA" sql-type="bigint"/>
        </property>
        <property name="codArt">
            <column name="COD_ART" sql-type="varchar" length="6"/>
        </property>
        <property name="noCia">
            <column name="NO_CIA" sql-type="varchar" length="2"/>
        </property>
        <property name="cantidad">
            <column name="CANTIDAD" sql-type="bigint"/>
        </property>
        <property name="codAlm">
            <column name="COD_ALM" sql-type="varchar" length="6"/>
        </property>
        <property name="precio">
            <column name="PRECIO" sql-type="double" precision="10" scale="2"/>
        </property>
        <property name="total">
            <column name="TOTAL" sql-type="double" precision="10" scale="2"/>
        </property>
        <property name="idcliente">
            <column name="IDCLIENTE" sql-type="bigint" not-null="true"/>
        </property>
        <many-to-one name="clienteByIdcliente" class="encens.khipus.model.purchases.Cliente">
            <column name="IDCLIENTE" not-null="true"/>
        </many-to-one>
        <many-to-one name="paquetesByIdpaquetes" class="encens.khipus.model.purchases.Paquetes">
            <column name="IDPAQUETES"/>
        </many-to-one>
    </class>
</hibernate-mapping>