<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Retencion" table="retencion" schema="" catalog="eos">
        <id name="idretencion">
            <column name="IDRETENCION" sql-type="bigint" not-null="true"/>
        </id>
        <property name="nombre">
            <column name="NOMBRE" sql-type="varchar" length="50"/>
        </property>
        <property name="porcentage">
            <column name="PORCENTAGE" sql-type="double" precision="5" scale="2"/>
        </property>
        <property name="idcliente">
            <column name="IDCLIENTE" sql-type="bigint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>