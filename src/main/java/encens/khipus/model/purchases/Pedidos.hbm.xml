<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Pedidos" table="pedidos" schema="" catalog="eos">
        <id name="idpedidos">
            <column name="IDPEDIDOS" sql-type="bigint" not-null="true"/>
        </id>
        <property name="descripcion">
            <column name="DESCRIPCION" sql-type="varchar" length="200"/>
        </property>
        <property name="estadoPedido">
            <column name="ESTADO_PEDIDO" sql-type="bigint" not-null="true"/>
        </property>
        <property name="tipoPedido">
            <column name="TIPO_PEDIDO" sql-type="varchar" length="10" not-null="true"/>
        </property>
        <property name="id">
            <column name="ID" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="fechaPedido">
            <column name="FECHA_PEDIDO" sql-type="date" not-null="true"/>
        </property>
        <property name="idempleado">
            <column name="IDEMPLEADO" sql-type="bigint" not-null="true"/>
        </property>
        <property name="iddireccion">
            <column name="IDDIRECCION" sql-type="bigint"/>
        </property>
        <property name="idzona">
            <column name="IDZONA" sql-type="bigint"/>
        </property>
        <property name="id1">
            <column name="ID1" sql-type="bigint" not-null="true"/>
        </property>
        <property name="total">
            <column name="TOTAL" sql-type="double" precision="10" scale="2" not-null="true"/>
        </property>
        <property name="fechaEntrega">
            <column name="FECHA_ENTREGA" sql-type="date"/>
        </property>
        <property name="fechaAPagar">
            <column name="FECHA_A_PAGAR" sql-type="date" not-null="true"/>
        </property>
        <property name="observacion">
            <column name="OBSERVACION" sql-type="varchar" length="100"/>
        </property>
        <property name="factura">
            <column name="FACTURA" sql-type="varchar" length="2" not-null="true"/>
        </property>
        <property name="supervisor">
            <column name="SUPERVISOR" sql-type="bigint"/>
        </property>
        <property name="distribuidor">
            <column name="DISTRIBUIDOR" sql-type="bigint"/>
        </property>
        <property name="porcenDescuento">
            <column name="PORCEN_DESCUENTO" sql-type="bigint"/>
        </property>
        <property name="porcenRetencion">
            <column name="PORCEN_RETENCION" sql-type="bigint"/>
        </property>
        <property name="tipopedidoIdtipopedido">
            <column name="TIPOPEDIDO_IDTIPOPEDIDO" sql-type="bigint" not-null="true"/>
        </property>
        <property name="idcliente">
            <column name="IDCLIENTE" sql-type="bigint" not-null="true"/>
        </property>
        <many-to-one name="clienteByIdcliente" class="encens.khipus.model.purchases.Cliente">
            <column name="IDCLIENTE" not-null="true"/>
        </many-to-one>
        <many-to-one name="empleadoByIdempleado" class="encens.khipus.model.purchases.Empleado">
            <column name="IDEMPLEADO" not-null="true"/>
        </many-to-one>
        <many-to-one name="estadoPedidosByEstadoPedido" class="encens.khipus.model.purchases.EstadoPedidos">
            <column name="ESTADO_PEDIDO" not-null="true"/>
        </many-to-one>
        <many-to-one name="tipopedidoByTipopedidoIdtipopedido" class="encens.khipus.model.purchases.Tipopedido">
            <column name="TIPOPEDIDO_IDTIPOPEDIDO" not-null="true"/>
        </many-to-one>
        <set name="articulosPedidosByIdpedidos" inverse="true">
            <key>
                <column name="PEDIDO" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.purchases.ArticulosPedido"/>
        </set>
    </class>
</hibernate-mapping>