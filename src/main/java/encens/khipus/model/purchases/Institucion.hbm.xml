<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Institucion" table="institucion" schema="" catalog="eos">
        <id name="idinstitucion">
            <column name="IDINSTITUCION" sql-type="bigint" not-null="true"/>
        </id>
        <property name="razonsocial">
            <column name="RAZONSOCIAL" sql-type="varchar" length="100" not-null="true"/>
        </property>
        <set name="clientesByIdinstitucion" inverse="true">
            <key>
                <column name="IDINSTITUCION" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.purchases.Cliente"/>
        </set>
    </class>
</hibernate-mapping>