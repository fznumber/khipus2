<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Distribuidor" table="distribuidor" schema="" catalog="eos">
        <id name="iddistribuidor">
            <column name="IDDISTRIBUIDOR" sql-type="bigint" not-null="true"/>
        </id>
        <property name="observacion">
            <column name="OBSERVACION" sql-type="varchar" length="50"/>
        </property>
        <set name="pedidosesByIddistribuidor" inverse="true">
            <key>
                <column name="IDDISTRIBUIDOR" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.purchases.Pedidos"/>
        </set>
    </class>
</hibernate-mapping>