<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Tipocliente" table="tipocliente" schema="" catalog="eos">
        <id name="idtipocliente">
            <column name="IDTIPOCLIENTE" sql-type="bigint" not-null="true"/>
        </id>
        <property name="nombre">
            <column name="NOMBRE" sql-type="varchar" length="50"/>
        </property>
        <set name="clientesByIdtipocliente" inverse="true">
            <key>
                <column name="IDTIPOCLIENTE" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.purchases.Cliente"/>
        </set>
    </class>
</hibernate-mapping>