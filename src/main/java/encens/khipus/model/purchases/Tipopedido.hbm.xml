<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.Tipopedido" table="tipopedido" schema="" catalog="eos">
        <id name="idtipopedido">
            <column name="IDTIPOPEDIDO" sql-type="bigint" not-null="true"/>
        </id>
        <property name="nombre">
            <column name="NOMBRE" sql-type="varchar" length="20"/>
        </property>
        <set name="pedidosesByIdtipopedido" inverse="true">
            <key>
                <column name="TIPOPEDIDO_IDTIPOPEDIDO" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="encens.khipus.model.purchases.Pedidos"/>
        </set>
    </class>
</hibernate-mapping>