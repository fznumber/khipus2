<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="encens.khipus.model.purchases.ArticulosPedido" table="articulos_pedido" schema="" catalog="eos">
        <id name="idarticulospedido">
            <column name="IDARTICULOSPEDIDO" sql-type="bigint" not-null="true"/>
        </id>
        <property name="idCuenta">
            <column name="ID_CUENTA" sql-type="bigint" not-null="true"/>
        </property>
        <property name="codArt">
            <column name="COD_ART" sql-type="varchar" length="6" not-null="true"/>
        </property>
        <property name="noCia">
            <column name="NO_CIA" sql-type="varchar" length="2" not-null="true"/>
        </property>
        <property name="id">
            <column name="ID" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="id1">
            <column name="ID1" sql-type="bigint" not-null="true"/>
        </property>
        <property name="cantidad">
            <column name="CANTIDAD" sql-type="bigint"/>
        </property>
        <property name="codAlm">
            <column name="COD_ALM" sql-type="varchar" length="6"/>
        </property>
        <property name="precio">
            <column name="PRECIO" sql-type="double" precision="10" scale="2"/>
        </property>
        <property name="reposicion">
            <column name="REPOSICION" sql-type="bigint"/>
        </property>
        <property name="total">
            <column name="TOTAL" sql-type="double" precision="10" scale="2"/>
        </property>
        <property name="promocion">
            <column name="PROMOCION" sql-type="bigint"/>
        </property>
        <property name="caja">
            <column name="CAJA" sql-type="bigint"/>
        </property>
        <property name="precioInv">
            <column name="PRECIO_INV" sql-type="bigint"/>
        </property>
        <property name="totalInv">
            <column name="TOTAL_INV" sql-type="bigint"/>
        </property>
    </class>
</hibernate-mapping>